  "scripts": {
    "build": "npm run test:lint:fix && npm run tsc",
    "tsc": "tsc --project tsconfig.compile.json",
    "tsc:watch": "tsc -w -p tsconfig.compile.json",
    "start": "docker-compose up",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:debug": "nodemon --inspect=0.0.0.0:9222 --watch ./dist/ --ignore './dist/**/*.d.ts' ./dist/index.js",
    "docker:init": "docker-compose build",
    "docker:stop": "docker container stop castelli-api",
    "test:lint": "eslint src --ext .ts",
    "test:lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier '{src,test}/**/*.ts' --write",
    "test": "npm run test:lint && npm run test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "typeorm": "tsed typeorm",
    "migration-generate": "tsed typeorm migration:create -n $NAME"
  },
